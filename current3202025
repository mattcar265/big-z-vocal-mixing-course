<script>
  document.addEventListener("DOMContentLoaded", function () {
    const customSection = document.querySelector(".customContent");
    const targetSection = document.getElementById("section_1564405816535_1") || document.getElementById("section_1564405797906_0");
    if (customSection && targetSection && targetSection.parentNode) {
      customSection.style.display = "block";
      targetSection.parentNode.insertBefore(customSection, targetSection.nextSibling);
    } else {
      console.error("Target section or customContent section not found.");
    }
  });
</script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    const customSection = document.querySelector(".customContent");
    const targetSection = document.getElementById("section_1564405816535_1") || document.getElementById("section_1564405797906_0");

    if (customSection && targetSection && targetSection.parentNode) {
      customSection.style.display = "block";
      targetSection.parentNode.insertBefore(customSection, targetSection.nextSibling);

      // Paths for play and pause SVG icons
      const playSolid = `<path fill-rule="evenodd" d="M4.5 5.653c0-1.427 1.529-2.33 2.779-1.643l11.54 6.347c1.295.712 1.295 2.573 0 3.286L7.28 19.99c-1.25.687-2.779-.217-2.779-1.643V5.653Z" clip-rule="evenodd" />`;
      const pauseSolid = `<path fill-rule="evenodd" d="M6.75 5.25a.75.75 0 0 1 .75-.75H9a.75.75 0 0 1 .75.75v13.5a.75.75 0 0 1-.75.75H7.5a.75.75 0 0 1-.75-.75V5.25Zm7.5 0A.75.75 0 0 1 15 4.5h1.5a.75.75 0 0 1 .75.75v13.5a.75.75 0 0 1-.75.75H15a.75.75 0 0 1-.75-.75V5.25Z" clip-rule="evenodd" />`;

      // Array to keep track of all waveforms
      const allWaveforms = [];

      function fadeToIcon(iconElement, newPath, container, isPauseState) {
        // Start fade-out animation
        iconElement.classList.remove("fade-in");
        iconElement.classList.add("fade-out");

        // Wait for fade-out to complete before swapping the icon
        setTimeout(() => {
          iconElement.innerHTML = newPath; // Update SVG path

          // Adjust the class based on pause/play state
          if (isPauseState) {
            container.classList.add("pause");
          } else {
            container.classList.remove("pause");
          }

          // Start fade-in animation for the new icon
          iconElement.classList.remove("fade-out");
          iconElement.classList.add("fade-in");
        }, 100); // Matches faster fade-out transition duration
      }

      const createWaveformContainer = (id, audioSrc) => {
        const container = document.createElement("div");
        container.className = "waveformContainer";

        const waveform = document.createElement("div");
        waveform.id = id;
        waveform.style.width = "100%";
        waveform.style.height = "120px";
        container.appendChild(waveform);

        // Create a parent div for the play button and SVG
        const playButtonContainer = document.createElement("div");
        playButtonContainer.className = "playButtonContainer";
        playButtonContainer.style.display = "flex";
        playButtonContainer.style.alignItems = "center";
        playButtonContainer.style.gap = "10px";

        // Add the button
        const button = document.createElement("div");
        button.className = "playPauseButton";
        playButtonContainer.appendChild(button);

        // Create the SVG element
        const svgIcon = document.createElementNS("http://www.w3.org/2000/svg", "svg");
        svgIcon.setAttribute("width", "40");
        svgIcon.setAttribute("height", "40");
        svgIcon.setAttribute("viewBox", "0 0 24 24");
        svgIcon.setAttribute("fill", "white");
        svgIcon.classList.add("size-6");
        svgIcon.innerHTML = playSolid; // Default to play icon

        playButtonContainer.appendChild(svgIcon);
        container.appendChild(playButtonContainer);

        customSection.querySelector(".soundwavesSection").appendChild(container);

        var linGrad = document.createElement('canvas').getContext('2d').createLinearGradient(0, 0, 1000, 128);
        linGrad.addColorStop(0, '#8C52FF');
        linGrad.addColorStop(1, '#FF914D');

        const wavesurfer = WaveSurfer.create({
          container: waveform,
          waveColor: "rgba(40, 47, 54, 1)",
          progressColor: id === "waveform1" ? "white" : linGrad,
          height: 55,
          responsive: true,
          barWidth: 2,
          cursorWidth: 0,
          hideScrollbar: true,
        });

        wavesurfer.load(audioSrc);

        // Add to the global waveforms array
        allWaveforms.push({ wavesurfer, svgIcon, playButtonContainer });

        // Handle play/pause logic
        playButtonContainer.addEventListener("click", () => {
          if (wavesurfer.isPlaying()) {
            // Pause the audio and update the icon to play
            fadeToIcon(svgIcon, playSolid, playButtonContainer, false);
            wavesurfer.pause();
          } else {
            // Pause all other waveforms and ensure their icons are updated
            allWaveforms.forEach(({ wavesurfer: otherWaveform, svgIcon: otherIcon, playButtonContainer: otherContainer }) => {
              if (otherWaveform !== wavesurfer && otherWaveform.isPlaying()) {
                otherWaveform.pause();
                fadeToIcon(otherIcon, playSolid, otherContainer, false);
              }
            });

            // Play the selected waveform and update the icon to pause
            fadeToIcon(svgIcon, pauseSolid, playButtonContainer, true);
            wavesurfer.play();
          }
        });

        // Reset the track when it ends and update to play icon
        wavesurfer.on("finish", () => {
          fadeToIcon(svgIcon, playSolid, playButtonContainer, false);
          wavesurfer.seekTo(0);
        });
      };

      // Create waveforms
      createWaveformContainer(
        "waveform1",
        "https://mattcar265.github.io/big-z-vocal-mixing-course/1%20Before%20Vocal%20Mixing.wav"
      );
      createWaveformContainer(
        "waveform2",
        "https://mattcar265.github.io/big-z-vocal-mixing-course/2%20After%20Vocal%20Mixing.wav"
      );
    } else {
      console.error("Target section or customContent section not found.");
    }

    // Titles remain untouched
    const waveform1 = document.querySelector("#waveform1");
    const waveform2 = document.querySelector("#waveform2");

    if (waveform1) {
      const floatingTitle1 = document.createElement("div");
      floatingTitle1.classList.add("soundwaveTitle");
      floatingTitle1.textContent = "Before Vocal Mixing";
      floatingTitle1.style.position = "absolute";
      floatingTitle1.style.top = "18px";
      floatingTitle1.style.left = "0px";
      floatingTitle1.style.color = "white";
      floatingTitle1.style.fontSize = "20px";
      floatingTitle1.style.zIndex = "10";
      waveform1.appendChild(floatingTitle1);
    } else {
      console.error("Waveform1 or its .wrapper class not found.");
    }

    if (waveform2) {
      const floatingTitle2 = document.createElement("div");
      floatingTitle2.classList.add("soundwaveTitle");
      floatingTitle2.textContent = "After Vocal Mixing";
      floatingTitle2.style.position = "absolute";
      floatingTitle2.style.top = "16px";
      floatingTitle2.style.left = "0px";
      floatingTitle2.style.color = "white";
      floatingTitle2.style.fontSize = "20px";
      floatingTitle2.style.zIndex = "10";
      waveform2.appendChild(floatingTitle2);
    } else {
      console.error("Waveform2 or its .wrapper class not found.");
    }
  });
</script>





<script>
  document.addEventListener("DOMContentLoaded", function () {
    const customSection = document.querySelector(".customContent");
    const mainContent = document.querySelector(".mainContent");

    function updateCustomContentHeight() {
      if (customSection && mainContent) {
        const mainContentHeight = mainContent.offsetHeight;
        customSection.style.height = `${300 + mainContentHeight}px`;
      }
    }

    // Update height initially with a delay to account for rendering
    setTimeout(updateCustomContentHeight, 100); // Adjust the delay as needed

    // Update height on window resize
    window.addEventListener("resize", updateCustomContentHeight);

    const videoContainer = document.getElementById('el_1737219486975_358');
    const videoSection = document.querySelector('.videoSection');
    const sectionToHide = document.getElementById('section_1737219486953_344');

    if (videoContainer && videoSection) {
      videoSection.appendChild(videoContainer);
    }

    if (sectionToHide) {
      sectionToHide.style.display = 'none';
    }
  });
</script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Get the elements with the specified IDs
    const sectionToMove1 = document.getElementById("section_1737223188216_339");

    // Get the buttonContainer element
    const buttonContainer = document.querySelector(".buttonContainer");

    if (sectionToMove1 && buttonContainer) {
      // Append the first section to buttonContainer
      buttonContainer.appendChild(sectionToMove1);

      // Find the element with the specified classes inside buttonContainer
      const targetElement = buttonContainer.querySelector(".col.span_12_of_12.standalone");

      // Remove the classes if the element exists
      if (targetElement) {
        targetElement.classList.remove("col", "span_12_of_12", "standalone");
      }
    }

    // Get the button element by its ID
    const button = document.getElementById("el_1737223361641_382");

    // Assign the class .tryButton to the button
    if (button) {
      button.classList.add("tryButton");
    }

    // Get the element by its ID
    const element = document.getElementById("el_1737223188239_354");

    // Assign the class enrollButton to the element
    if (element) {
      element.classList.add("enrollButton");
    }

    // Get the element by its ID
    const element1 = document.getElementById("el_1737223361642_383");

    // Change the content of the element
    if (element1) {
      element1.textContent = "Try the First Hour Free";
    }
  });

  document.addEventListener("DOMContentLoaded", function () {
    // Select all elements with the playButtonContainer class
    const playButtons = document.querySelectorAll(".playPauseButton");

    // Select all elements with the _accordionHeader class
    const accordionHeaders = document.querySelectorAll("._accordionHeader");

    // Function to add touch and click events
    function addTouchEvents(elements) {
      elements.forEach((element) => {
        element.addEventListener("touchstart", handleTouchStart, { passive: true });
        element.addEventListener("touchend", handleTouchEnd);
        element.addEventListener("click", handleTouchStart); // Fallback for non-touch devices
      });
    }

    // Add events to both play buttons and accordion headers
    addTouchEvents(playButtons);
    addTouchEvents(accordionHeaders);

    // Event handler for touchstart
    function handleTouchStart(event) {
      console.log("Touchstart fired", event.currentTarget);
      const element = event.currentTarget;
      console.log("ELEMENT:" + element);
      element.classList.add("active");
    }

    // Event handler for touchend
    function handleTouchEnd(event) {
      console.log("Touchend fired", event.currentTarget);
      const element = event.currentTarget;
      setTimeout(() => {
        element.classList.remove("active");
      }, 150); // Adjust to match CSS transition timing
    }
});


</script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Select all accordion sections
    const accordionSections = document.querySelectorAll(".accordion-section");

    accordionSections.forEach((section) => {
      const header = section.querySelector(".accordion-header");
      const content = section.querySelector(".accordion-content");

      // Add a click event listener to the header
      if (header) {
        header.addEventListener("click", () => {
          // Close all other accordion sections
          accordionSections.forEach((otherSection) => {
            if (otherSection !== section) {
              const otherContent = otherSection.querySelector(".accordion-content");
              otherSection.classList.remove("active");
              if (otherContent) {
                otherContent.style.maxHeight = null; // Collapse content
              }
            }
          });

          // Toggle the current section
          const isActive = section.classList.contains("active");
          if (isActive) {
            section.classList.remove("active");
            content.style.maxHeight = null; // Collapse content
          } else {
            section.classList.add("active");
            content.style.maxHeight = content.scrollHeight + "px"; // Expand content
          }
        });
      }
    });
  });

  // Navigate to Payment
  function navigateToPayment() {
    window.location.href = "https://www.bigzcourses.com/payment?product_id=vocal-mixing";
  }

  document.addEventListener("DOMContentLoaded", function () {
    const accordionHeaders = document.querySelectorAll(".accordion-header");

    accordionHeaders.forEach((header) => {
      header.addEventListener("touchstart", () => {
        header.classList.add("touch-active");
      });

      header.addEventListener("touchend", () => {
        setTimeout(() => {
          header.classList.remove("touch-active");
        }, 300); // Adjust delay to match CSS transition duration
      });

      header.addEventListener("touchcancel", () => {
        header.classList.remove("touch-active");
      });
    });

    document.querySelector('.signUpLink').addEventListener('click', function () {
      const realButton = document.getElementById('el_1737223361641_382');
      if (realButton) {
        realButton.click();
      }
    });
  });
</script>

<script>
  document.addEventListener("DOMContentLoaded", () => {
    const priceInfoElements = document.querySelectorAll(".priceInfo > p");
    const priceInfo = document.querySelector(".priceInfo");

    const discountEndDate = new Date("2025-02-25T22:00:00Z"); // Example end date

    const updateCountdown = () => {
      const currentDate = new Date();
      const timeDifference = discountEndDate - currentDate; // Milliseconds remaining

      priceInfoElements.forEach((priceInfoElement) => {
        if (timeDifference <= 0) {
          priceInfo.innerHTML = ""; // Set end message
          return;
        }

        const days = Math.floor(timeDifference / (1000 * 60 * 60 * 24));
        const hours = Math.floor((timeDifference % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        const minutes = Math.floor((timeDifference % (1000 * 60 * 60)) / (1000 * 60));
        const seconds = Math.floor((timeDifference % (1000 * 60)) / 1000);

        priceInfoElement.innerHTML = `Time left:&nbsp<strong>${days}</strong>&nbsp;days&nbsp;<strong>${hours}</strong>&nbsp;hrs&nbsp;<strong>${minutes}</strong>&nbsp;min&nbsp;<strong>${seconds}</strong>&nbsp;sec`;


      });
    };

    setInterval(updateCountdown, 1000);
    updateCountdown();
  });
</script>
